<*+ CSTDLIB  *>
<*+ NOHEADER *>
<*+ M2EXTENSIONS *>

DEFINITION MODULE [2] times;

IMPORT time;

TYPE
  clock_t = time.clock_t;

<* IF __GEN_C__ THEN *> <*- GENTYPEDEF *> <* END *>
  tms = RECORD
    tms_utime:  clock_t;
    tms_stime:  clock_t;
    tms_cutime: clock_t;
    tms_cstime: clock_t;
  END;

PROCEDURE times (VAR tp: tms): clock_t;

END times.
